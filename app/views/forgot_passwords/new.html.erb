<%= form_tag forgot_passwords_path, class: "sing_in" do %>
   <h1> Forgot Password? </h1>
   <p> We will send you an email with a link that you can use to reset your password </p>
    <%= label_tag :email, "Email Address" %>
    <%= email_field_tag :email %>
  <%= submit_tag "Send Email", class: "btn btn-default" %>
<% end %>
